<div class="center jumbotron">
<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>
<div class="city-block">
<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th class="col-md-2">Feedback type</th>
      <th class="col-md-2">Feedback subject</th>
      <th class="col-md-2">Feedback email</th>
      <th class="col-md-2">Feedback mno</th>
      <th class="col-md-2">Feedback comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td class="col-md-2"><%= feedback.feedback_type %></td>
        <td class="col-md-2"><%= feedback.feedback_subject %></td>
        <td class="col-md-2"><%= feedback.feedback_email %></td>
        <td class="col-md-2"><%= feedback.feedback_mno %></td>
        <td class="col-md-2"><%= feedback.feedback_comment %></td>
        <td class="col-md-4">
        <%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Feedback', new_feedback_path , class: "btn btn-info"  %>
</div>